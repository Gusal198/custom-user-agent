{
	"manifest_version": 2,
	"name": "Custom User-Agent",
	"version": "2.0",
	"version_name": "2.0-alpha.1",
	"default_locale": "en",
	"icons": {
        "16": "assets/icons/16.png",
        "32": "assets/icons/32.png",
        "48": "assets/icons/48.png",
        "128": "assets/icons/128.png"
    },
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"default_popup": "index.html"
	},
	"options_page": "options.html",
	"permissions": [
		"storage",
		"<all_urls>",
		"webNavigation",
		"webRequest",
		"webRequestBlocking"
	]
}
